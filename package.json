{
  "name": "datasheet-server",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon -w src --exec \"babel-node src\"",
    "build": "cross-env NODE_ENV=production npx babel src -d dist",
    "start": "node dist",
    "lint": "standard \"src/**/*.js\" \"test/**/*.js\"",
    "test-watch": "ava --watch",
    "test": "ava --verbose",
    "travis-encrypt": "./scripts/encrypt.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.5.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.5",
    "geojson": "^0.5.0",
    "googleapis": "^100.0.0",
    "morgan": "^1.8.0",
    "mz": "^2.7.0",
    "node-fetch": "^3.2.4",
    "object-hash": "^3.0.0",
    "ramda": "^0.28.0",
    "resource-router-middleware": "^0.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/node": "^7.17.10",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "ava": "4.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "2.0.16",
    "standard": "^17.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme"
}
